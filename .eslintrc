{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {},
  "extends": "airbnb-base",
  "plugins": [],
  "rules": {
    "no-param-reassign": ["error", {
      "props": false
    }],
    "no-warning-comments": "warn",
    "max-len": ["error", {
      "code": 100
    }],
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": ["webpack.*.js"]
    }],
    "no-underscore-dangle": "error"
  }
}
